{
    "dashboard": {
        "title": "Data Quality Auto-Healing System",
        "tags": [
            "data-quality",
            "mlops",
            "monitoring"
        ],
        "timezone": "browser",
        "schemaVersion": 16,
        "version": 1,
        "refresh": "5s",
        "panels": [
            {
                "id": 1,
                "title": "Quality Score Over Time",
                "type": "graph",
                "gridPos": {
                    "x": 0,
                    "y": 0,
                    "w": 12,
                    "h": 8
                },
                "targets": [
                    {
                        "expr": "dq_quality_score",
                        "legendFormat": "{{pipeline_id}}"
                    }
                ],
                "yaxes": [
                    {
                        "format": "short",
                        "min": 0,
                        "max": 100
                    }
                ]
            },
            {
                "id": 2,
                "title": "Issues Detected by Type",
                "type": "piechart",
                "gridPos": {
                    "x": 12,
                    "y": 0,
                    "w": 6,
                    "h": 8
                },
                "targets": [
                    {
                        "expr": "sum by (issue_type) (increase(dq_issues_detected_total[1h]))",
                        "legendFormat": "{{issue_type}}"
                    }
                ]
            },
            {
                "id": 3,
                "title": "Remediation Success Rate",
                "type": "stat",
                "gridPos": {
                    "x": 18,
                    "y": 0,
                    "w": 6,
                    "h": 8
                },
                "targets": [
                    {
                        "expr": "sum(rate(dq_remediations_total{status='completed'}[5m])) / sum(rate(dq_remediations_total[5m])) * 100"
                    }
                ],
                "options": {
                    "unit": "percent",
                    "thresholds": {
                        "steps": [
                            {
                                "value": 0,
                                "color": "red"
                            },
                            {
                                "value": 80,
                                "color": "yellow"
                            },
                            {
                                "value": 95,
                                "color": "green"
                            }
                        ]
                    }
                }
            },
            {
                "id": 4,
                "title": "Mean Time To Recovery (MTTR)",
                "type": "graph",
                "gridPos": {
                    "x": 0,
                    "y": 8,
                    "w": 12,
                    "h": 8
                },
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(dq_mttr_seconds_bucket[5m]))",
                        "legendFormat": "p95 MTTR"
                    },
                    {
                        "expr": "histogram_quantile(0.50, rate(dq_mttr_seconds_bucket[5m]))",
                        "legendFormat": "p50 MTTR"
                    }
                ],
                "yaxes": [
                    {
                        "format": "s"
                    }
                ]
            },
            {
                "id": 5,
                "title": "Detector Accuracy",
                "type": "bargauge",
                "gridPos": {
                    "x": 12,
                    "y": 8,
                    "w": 6,
                    "h": 8
                },
                "targets": [
                    {
                        "expr": "dq_detector_accuracy",
                        "legendFormat": "{{detector}}"
                    }
                ],
                "options": {
                    "orientation": "horizontal",
                    "displayMode": "gradient",
                    "minVizWidth": 0,
                    "minVizHeight": 10
                }
            },
            {
                "id": 6,
                "title": "Active Remediations",
                "type": "stat",
                "gridPos": {
                    "x": 18,
                    "y": 8,
                    "w": 6,
                    "h": 8
                },
                "targets": [
                    {
                        "expr": "sum(dq_remediations_total{status='started'}) - sum(dq_remediations_total{status='completed'})"
                    }
                ],
                "options": {
                    "colorMode": "value",
                    "graphMode": "area"
                }
            },
            {
                "id": 7,
                "title": "Profiling Performance",
                "type": "graph",
                "gridPos": {
                    "x": 0,
                    "y": 16,
                    "w": 12,
                    "h": 8
                },
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(dq_profiling_duration_seconds_bucket[5m]))",
                        "legendFormat": "p95 Profiling Time"
                    }
                ],
                "yaxes": [
                    {
                        "format": "s"
                    }
                ]
            },
            {
                "id": 8,
                "title": "Detection Latency",
                "type": "heatmap",
                "gridPos": {
                    "x": 12,
                    "y": 16,
                    "w": 12,
                    "h": 8
                },
                "targets": [
                    {
                        "expr": "sum(rate(dq_detection_latency_seconds_bucket[5m])) by (le, detector)"
                    }
                ],
                "options": {
                    "calculate": true,
                    "yAxis": {
                        "format": "s"
                    }
                }
            }
        ]
    }
}